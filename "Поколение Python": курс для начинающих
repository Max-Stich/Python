# "Самописный калькулятор"

a, b, c = int(input()), int(input()), int(input())

if b == 0 and c == '/':
	print('На ноль делить нельзя!')
elif c == '+':
	print(a + b)
elif c == '-':
	print(a - b)
elif c == '*':
	print(a * b)
elif c == '/' and b != 0:
	print(a / b)
elif c != ('+', '-', '*', '/'):
	print('Неверная операция')
  
##########################################

# Цвет колеса рулетки

a = int(input())
if a > 36 or a < 0:
	print('ошибка ввода')
else: 
	if a == 0:
		print('зеленый')
	elif a <= 1 or a <= 10 and (a % 2 > 0):
		print('красный')
	elif a <= 1 or a <= 10 and (a % 2 == 0):
		print('черный')
	elif a <= 11 or a <= 18 and (a % 2 > 0):
		print('черный')
	elif a < 11 or a <= 18 and (a % 2 == 0):
		print('красный')
	elif a <= 19 or a <= 28 and (a % 2 > 0):
		print('красный')
	elif a <= 19 or a <= 28 and (a % 2 == 0):
		print('черный')
	elif a <= 29 or a <= 36 and (a % 2 > 0):
		print('черный')
	elif a <= 29 or a <= 36 and (a % 2 == 0):
		print('красный')
    
##########################################

# Пересечение отрезков

a1 = int(input())
b1 = int(input())
a2 = int(input())
b2 = int(input())

if a2 > b1 or a1 > b2:
    print('пустое множество')
elif a1 == b2:
    print(a1)
elif a2 == b1:
    print(a2)
else:
    if a1 > a2: 
        a2 = a1
    if b1 < b2:
        b2 = b1
    print(a2, b2)
    

